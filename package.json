{
  "name": "doubledash",
  "version": "1.0.0",
  "description": "Double Dash is simple utility library meant to help transition people off of jQuery.",
  "type": "module",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "css": "npm-run-all css-compile css-compress",
    "css-compile": "sass src/animate/css/animate.scss build/animate.css",
    "css-compress": "sass build/animate.css build/animate.min.css --style compressed",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "docs": "documentation --markdown-toc=false build src/event/index.js -f md > doc.md"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jest-environment-node",
    "transform": {},
    "setupFiles": [
      "./test/config/mocks/dom.js"
    ]
  },
  "author": "Demetri Mihalakakos",
  "license": "ISC",
  "devDependencies": {
    "documentation": "^13.2.5",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.34.0"
  },
  "dependencies": {}
}
